{
    "name": "cdp-vue",
    "version": "1.0.5",
    "description": "CDP Components for Vue.js",
    "main": "dist/index.min.js",
    "files": [
        "dist",
        "src",
        "readme.md",
        "package.json"
    ],
    "scripts": {
        "prepush": "npm run lint && npm run build",
        "dev": "npm run gen:doc && cross-env NODE_ENV=development node ./build/server.js",
        "start": "npm run build:doc && cross-env NODE_ENV=production pm2 start ./build/server.js",
        "lint": "NODE_ENV=production eslint --quiet --ext .js,.vue ./src",
        "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./build/webpack.src.js && cross-env NODE_ENV=production webpack --config ./build/webpack.src.js",
        "build:doc": "npm run gen:doc && rimraf dist-doc && npm run build:client && npm run build:server",
        "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js --progress --hide-modules",
        "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --progress --hide-modules",
        "gen:doc": "node ./tools/gen-doc.js"
    },
    "repository": {
        "type": "git",
        "url": "git+http://gitlab.cdpyun.com/ellly.qian/cdp-vue.git"
    },
    "author": "CDP FrontEnd Team",
    "bugs": {
        "url": "http://gitlab.cdpyun.com/ellly.qian/cdp-vue/issues"
    },
    "homepage": "https://aimi-test.ecosaas.com/iso/index",
    "directories": {
        "test": "test"
    },
    "private": true,
    "engines": {
        "node": ">=6.0",
